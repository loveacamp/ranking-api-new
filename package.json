{
    "name": "ranking-api",
    "version": "1.0.0",
    "main": "index.js",
    "author": "Leandro Willian Fraccaroli <leandro.fraccaroli@edu.unipar.br>",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts",
        "build": "tsc",
        "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
        "migrate:run": "yarn typeorm migration:run -d ./src/database/",
        "migrate:create": "yarn typeorm migration:create ./src/database/migrations/nome-aqui",
        "migrate:revert": "yarn typeorm migration:revert -d ./src/database/",
        "prettier": "prettier --write \"**/*.{ts, js}\""
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/node": "^18.15.5",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "eslint": "7.21.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.8.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.2"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "jsonwebtoken": "^9.0.0",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.12"
    }
}